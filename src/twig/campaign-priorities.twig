{% extends "campaign-top-level.twig" %}

{% set has_page_image = false %}

{% block layout_header %}
  {% include 'partials/campaign-header.twig' with {
    is_block: true,
    dark: true
  } %}
{% endblock %}

{% block page_header %}
  {% include 'partials/page-header.twig' with {
    title: 'Our Priorities',
    image_src: has_page_image
  } %}
{% endblock %}

{% block page %}
  {% include 'paragraphs/image-with-text.twig' with {
    right: true,
    color: "access",
    title: "Access"
  } %}
  {% include 'paragraphs/image-with-text.twig' with {
    color: "academic-excellence",
    title: "Academic Excellence"
  }%}
  {% include 'paragraphs/image-with-text.twig' with {
    right: true,
    color: "experience",
    title: "Experience"
  } %}
  {% include 'paragraphs/two-col-linked-list.twig' %}
{% endblock %}