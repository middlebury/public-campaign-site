{% extends "layout.twig" %}

{% set has_page_image = true %}

{% set page_header = {
  title: 'Argentina',
  breadcrumb: [
    {
      text: 'Middlebury C.V. Star Schools Abroad'
    },
    {
      text: 'Our Schools'
    }
  ],
  image_src: has_page_image,
  has_menu: true
} %}

{% block layout_header %}
  {% include 'partials/campaign-header.twig' %}
{% endblock %}

{% block page %}
  {% include 'partials/top-level-layout.twig' with {
    show_menu: true,
    show_lead: true,
    has_page_image: has_page_image,
    theme: 'experience'
  } %}  
{% endblock %} 

{% block layout_footer %}
    {% include 'partials/campaign-footer.twig' %}
{% endblock %}
